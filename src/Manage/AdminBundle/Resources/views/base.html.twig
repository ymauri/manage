    </head>
    <body class="page-header-fixed page-quick-sidebar-over-content" >
    <div class="loader-page"></div>
        {% block header%}
        {% endblock %}
        <div class="clearfix"> </div>
        <div class="page-container">
            {% block sidebar%}
            {% endblock %}
            <div class="page-content-wrapper">
                <div class='page-content' style="min-height:auto">
                    <div class="hidden-xs" >
                        <div id="dashboard-report-range" class=" pull-right tooltips btn btn-sm btn-default" style="cursor: default; border: none; background: #D5DADE;" >
                            <i class="icon-calendar"></i>&nbsp; <span class=" visible-lg-inline-block visible-md-inline-block visible-sm-inline-block">{{ 'today' | date('l, M d, Y') }} </span>&nbsp;
                        </div>
                        <br/>
                        <br/>
                        <br/>
                    </div>
                    <div class="global-msg alert alert-success" hidden="hidden"></div>
                    <div class="global-msg alert alert-danger" hidden="hidden"></div>
                    {%for msg in app.session.flashbag.get('error') %}
                        <div class="global-msg alert alert-danger">
                            {{ msg }}
                        </div>
                        <script>
                            setTimeout(function(){$('.alert-danger').hide(300)}, 4400)
                        </script>
                    {% endfor %}
                    {%for msg in app.session.flashbag.get('success') %}
                    <div class="global-msg alert alert-success">
                        {{ msg }}
                    </div>
                    <script>
                        setTimeout(function(){$('.alert-success').hide(300)}, 4400)
                    </script>
                    {% endfor %}

                {% block body%}                        
                {% endblock %}
                    <div class="modal fade" id="portlet-config" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                    <h4 class="modal-title">Modal title</h4>
                                </div>
                                <div class="modal-body">
                                    Widget settings form goes here
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn blue">Save changes</button>
                                    <button type="button" class="btn default" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                        <!-- /.modal-dialog -->
                    </div>
                </div>
            </div>
        </div>
        {% block footer%}                        
        {% endblock %}
       
